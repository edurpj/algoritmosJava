package estruturadados.vetor.teste;

import java.util.ArrayList;

public class Aula12 {

	public static void main(String[] args) {
		
		
		ArrayList<String> arrayList = new ArrayList<String>();
		
		arrayList.add("A");
		arrayList.add("B");
		arrayList.add("C");
		
		System.out.println(arrayList);
		
		arrayList.add(1, "D");
		System.out.println(arrayList);
		

	}

}
